{
    "name": "kreait/personio",
    "description": "An SDK to work with the Personio API",
    "keywords": ["personio", "api", "sdk"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérôme Gamez",
            "email": "jerome@gamez.name"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0",
        "guzzlehttp/promises": "^1.3",
        "myclabs/php-enum": "^1.6",
        "psr/http-message": "^1.0",
        "symfony/options-resolver": "^4.1",
        "tightenco/collect": "^5.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpunit/phpunit": "^7.3",
        "symfony/var-dumper": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Kreait\\Personio\\": "src/Personio"
        },
        "files": [
            "src/Personio.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kreait\\Personio\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": [
            "vendor/bin/php-cs-fixer fix"
        ],
        "test": [
            "vendor/bin/phpunit --testdox"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
